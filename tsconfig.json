{
    "compilerOptions": {
        "esModuleInterop": true, // needed for fast-object-stringify
        "target": "es6",
        "jsx": "react",
        "moduleResolution": "node",
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
        // Don't typecheck stuff internal to node_modules
        "skipLibCheck": true,
    }
}